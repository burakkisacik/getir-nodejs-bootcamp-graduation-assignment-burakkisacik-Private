{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "plugins": ["prettier"],
  "rules": {
    "no-console": "off",
    "radix": "off",
    "eqeqeq": "off",
    "no-restricted-globals": "off",
    "no-unused-vars": "off",
    "no-use-before-define": "off",
    "dot-notation": "off",
    "arrow-body-style": "off",
    "consistent-return": "off",
    "prefer-arrow-callback": "off",
    "no-underscore-dangle": "off",
    "no-undef": "off"
  }
}
